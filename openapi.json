{
  "openapi": "3.1.0",
  "info": {
    "title": "Casual Trading Data API",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://raw.githubusercontent.com"
    }
  ],
  "paths": {
    "/jackmmaher/casual-trading-gpt/main/Casual_Trading_Mapping.geojson": {
      "get": {
        "operationId": "getTradingData",
        "summary": "Get casual trading site data",
        "responses": {
          "200": {
            "description": "A list of casual trading sites",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "features": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "properties": {
                            "type": "object",
                            "properties": {
                              "Location_Desc": { "type": "string" },
                              "Symbol_Type": { "type": "string" },
                              "Description": { "type": "string" },
                              "Area_LEA": { "type": "string" },
                              "Lat": { "type": "number" },
                              "Long": { "type": "number" }
                            }
                          },
                          "geometry": {
                            "type": "object",
                            "properties": {
                              "coordinates": {
                                "type": "array",
                                "items": { "type": "number" }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
